{
  "name": "rikudou/skqrpayment",
  "type": "library",
  "description": "QR payment library for Slovak accounts",
  "homepage": "https://github.com/RikudouSage/QrPaymentSK",
  "keywords": [
    "qr",
    "payment"
  ],
  "license": "WTFPL",
  "authors": [
    {
      "name": "Dominik Chrástecký",
      "email": "dominik@chrastecky.cz"
    }
  ],
  "require": {
    "php": ">=5.5.0"
  },
  "autoload": {
    "classmap": [
      "QrPayment.php",
      "QrPaymentException.php",
      "QrPaymentOptions.php",
      "IBANtoBIC.php"
    ]
  },
  "suggest": {
    "endroid/qr-code": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^4"
  },
  "autoload-dev": {
    "psr-4": {
      "\\rikudou\\SkQrPayment\\Tests\\": "tests/"
    }
  }
}
